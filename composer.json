{
    "name": "platon.ai/official_website",
    "description": "Platon AI Officail Home",
    "homepage": "http://platon.ai",
    "type": "project",
    "license": "Apache 2.0",
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "psy/psysh": "@stable"
    },
    "suggest": {
        "phpunit/phpunit": "Allows automated tests to be run without system-wide install."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
